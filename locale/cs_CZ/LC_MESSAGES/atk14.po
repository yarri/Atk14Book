#: ../load.php:37
msgid "Service Unavailable"
msgstr "Služba je nedostupná"

#: ../load.php:38
msgid "This site is in maintenance. Please come back later."
msgstr "Probíhá údržba této stránky. Přijďte prosím později."

#: ../src/atk14/atk14_form.php:247
msgid "The form contains errors, therefore it cannot be processed."
msgstr "Formulář obsahuje chyby, proto nemůže být zpracován."

#: ../src/atk14/atk14_form.php:539
msgid "Please, submit the form again"
msgstr "Prosím, odešlete formulář znovu."

#: ../src/atk14/atk14_locale.php:71
msgid "atk14.date_format"
msgstr "j. n. Y"

#: ../src/atk14/atk14_locale.php:92
msgid "atk14.parse_date_pattern"
msgstr ""
"/^(?<day>[0-9]{1,2})\\.[  ]*(?<month>[0-9]{1,2})\\.[  ]*(?<year>[0-9]{4})$/"

#: ../src/atk14/atk14_locale.php:127
msgid "atk14.datetime_format"
msgstr "j. n. Y H:i"

#: ../src/atk14/atk14_locale.php:141
msgid "atk14.parse_datetime_pattern"
msgstr ""
"/^(?<day>[0-9]{1,2})\\.[  ]*(?<month>[0-9]{1,2})\\.[  ]*(?<year>[0-9]"
"{4})\\s+(?<hours>[0-9]{2}):(?<minutes>[0-9]{2})$/"

#: ../src/atk14/atk14_locale.php:155
msgid "atk14.datetime_with_seconds_format"
msgstr "j. n. Y H:i:s"

#: ../src/atk14/atk14_locale.php:168
msgid "atk14.parse_datetime_with_seconds_pattern"
msgstr ""
"/^(?<day>[0-9]{1,2})\\.[  ]*(?<month>[0-9]{1,2})\\.[  ]*(?<year>[0-9]{4}) (?"
"<hours>[0-9]{2}):(?<minutes>[0-9]{2}):(?<seconds>[0-9]{2})$/"

#: ../src/atk14/atk14_locale.php:185
msgid "atk14.number_format.decimal_point"
msgstr ","

#: ../src/atk14/atk14_locale.php:200
msgid "atk14.number_format.thousands_separator"
msgstr " "

#: ../src/atk14/atk14_sorting.php:167
msgid "Sort table by this column"
msgstr "Třídit tabulku podle tohoto sloupce"

#: ../src/atk14/helpers/block.a_destroy.php:42
msgid "Are you sure?"
msgstr "Jste si jistý?"

